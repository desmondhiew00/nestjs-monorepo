{
  "name": "nest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nest-project/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format:check": "prettier --check \"src//*.ts\" \"test//*.ts\"",
    "lint:nofix": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:inspect": "eslint --inspect-config",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nest-project/test/jest-e2e.json",
    "prisma:generate": "rimraf generated/graphql && npx prisma generate && ts-node scripts/executor/parse-gql-generated",
    "migration:gen": "npx prisma migrate dev --create-only ",
    "migration:run": "npx prisma migrate deploy",
    "db:seed": "ts-node scripts/executor/seeder",
    "codegen:crud-module": "ts-node scripts/generator/codegen/crud-module"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@aws-sdk/client-s3": "^3.675.0",
    "@aws-sdk/lib-storage": "^3.675.0",
    "@aws-sdk/s3-request-presigner": "^3.675.0",
    "@fastify/compress": "7.0.3",
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/static": "7.0.4",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.4.5",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.5",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/platform-fastify": "^10.4.13",
    "@nestjs/swagger": "^7.4.2",
    "@paljs/plugins": "^7.0.2",
    "@prisma/client": "^5.21.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "fastify": "^4.28.1",
    "graphql": "^16.9.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "inquirer": "^12.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma-extension-soft-delete": "^2.0.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "signale": "^1.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.2",
    "@nestjs/testing": "^10.4.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.12",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/pluralize": "^0.0.33",
    "@types/signale": "^1.4.7",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "chalk": "5.3.0",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^28.8.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "jest": "^29.5.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.21.1",
    "prisma-nestjs-graphql": "^20.0.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@app/aws(|/.*)$": "<rootDir>/libs/aws/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1"
    }
  }
}
